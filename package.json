{
  "name": "ProfitPAD",
  "version": "1.0.0",
  "description": "An App to help flippers keep track of workflow.",
  "main": "server.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "my-app": "yarn start --prefix my-app",
    "dev": "concurrently \"yarn run server\" \"yarn run my-app\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn install --prefix my-app && yarn run build --prefix my-app"
  },
  "author": "Tanner Noble <tannernoble@gmail.com> (http://www.tannernoble.com)",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.2",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
